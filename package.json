{
	"name": "epsilon",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"dev:api": "poetry --directory ./backend run uvicorn src.main:app --reload",
		"dev:app": "cargo run --manifest-path=./src-tauri/Cargo.toml tauri dev",
		"build": "vite build",
		"preview": "vite preview",
		"sync:api": "node ./scripts/sync-api.mjs",
		"sync:code": "poetry --directory ./backend run python3 ./scripts/consolidate-code.py",
		"sync:types": "svelte-kit sync",
		"db:revision": "poetry --directory ./backend run alembic -c ./backend/alembic.ini revision --autogenerate",
		"db:migrate": "poetry --directory ./backend run alembic -c ./backend/alembic.ini upgrade head",
		"test": "poetry --directory ./backend run python -m pytest -vv ./backend/tests && bun run test:integration && bun run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --fix . && poetry --directory ./backend run ruff check --fix --config ./backend/pyproject.toml",
		"format": "prettier --write --list-different . && poetry --directory ./backend run ruff format --config ./backend/pyproject.toml",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "svelte-kit sync && husky",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"chromatic": "npx chromatic"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
